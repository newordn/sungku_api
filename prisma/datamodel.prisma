type User {
   id: ID! @id
   name:String
   phone: String! @unique
   password: String
   code: String
   account : Account @relation(link: TABLE)
}
type Account {
   id: ID! @id
   balance: Float!
   owner : User!
   createdAt: DateTime! @createdAt
   transactions: [Transaction!]! @relation(name:"AccountTransactions")
}

type Transaction{
   id: ID! @id
   amount : Float!
   type : String!
   createdAt: DateTime! @createdAt
   initiator: Account! @relation(name:"AccountTransactions")
   receiver : ID!
}